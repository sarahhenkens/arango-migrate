{
  "name": "arango-migrate",
  "description": "Tool to perform schema migrations for an ArangoDB database",
  "version": "0.0.1",
  "author": "Sarah Henkens @sarahhenkens",
  "bin": {
    "arango-migrate": "./bin/run"
  },
  "bugs": "https://github.com/sarahhenkens/arango-migrate/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "arangojs": "^7.5.0",
    "joi": "^17.4.2",
    "tslib": "^1",
    "umzug": "^3.0.0-beta.16"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/sarahhenkens/arango-migrate",
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "arango-migrate",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "targets": [ "node14-linux-arm64" ],
    "outputPath": "dist"
  }
}
